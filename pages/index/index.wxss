

.title {
    /* margin-top: 40rpx; */
    background: #61a1ff;
    color: white;
    line-height: 60rpx;
    text-align: center;
    border:1px solid #538bdf;    
    border-top-color: #84a5d7;
    border-radius: 4px;
    margin: 0 24rpx;
}

.tips{
  padding: 20rpx ;
  text-align: center;
  font-size: 25rpx;
}

.flow-process {
    display: -webkit-flex;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 0 24rpx;
}

.flow-process-item {
    flex-grow: 3;
    text-align: center;
    padding: 12rpx 0;
    background: #fafafa;
    border-radius: 4px;
    box-shadow: 0 0 1px 1px #e4e4e4;
}
.flow-process-index {
    display: block;
    font-size: 30rpx;
    color: #61a1ff;
}

.flow-process-title {
    display: block;
    font-size: 24rpx;
    color: #7f7f7f;
}

.flow-process-arrow {
    flex-grow: 1.5;
    width: 17rpx;
    height: 34rpx;
    background: url("http://www.gzyueyun.com/m/yueyun-mini-app/assets/img/flow_process_right_arrow.png") no-repeat center/contain;
}

