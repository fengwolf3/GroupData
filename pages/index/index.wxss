    background: white;
    font-size: 28rpx;
    font-family:微软雅黑;
    padding-left: 42rpx;
    padding-bottom: 0;
}

.input-group {
    position: relative;
    padding:10rpx 0;
    border-bottom: 1px solid #f3f3f3;
}

.input-group label {
    display: inline-block;
    width: 28%;
    
}

.input-group input{
    display: inline-block;
    border: none;
    width: 55%;
    height: auto;
    min-height: auto;
    overflow: inherit;
    padding-left: 5px;
}



.input-group textarea {
    display: inline-block;
    width: 65%;
    vertical-align: top;
    height: 60rpx;
    padding-left: 5px;
}

.input-group .error-icon {
    position: absolute;
    top: 50%;
    right: 8%;
    margin-top: -8px;
}

.input-group.error input, .input-group.error textarea {
    outline: 1px solid red;
}

